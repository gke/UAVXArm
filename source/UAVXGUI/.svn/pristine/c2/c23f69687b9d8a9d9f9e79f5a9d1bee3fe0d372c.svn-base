            AltCompT * 0.001 + ",");

            for (i = 0; i < MotorsAndServos; i++)
               SaveTextLogFileStreamWriter.Write(PWMT[i] + ",");

            SaveTextLogFileStreamWriter.Write("Nav," +
            GPSMissionTimeT + "," +
            NavStateTimeoutT + "," +
            NavStateT + "," +
            FailStateT + "," +

            GPSNoOfSatsT + "," +
            GPSFixT + "," +
            GPShAccT * 0.01 + "," +

            GPSVelT * 0.1 + "," +
            GPSHeadingT * 0.001 + "," +
            GPSRelAltitudeT * 0.01 + "," +
            GPSLatitudeT * 1e-7 + "," +
            GPSLongitudeT * 1e-7 + "," +

            NavSensitivityT * 0.1 + "," +

            CurrWPT + "," +

            WPBearingT * 0.001 + "," +

            CruiseThrottleT * 0.001 + "," +
            RangefinderAltitudeT * 0.01 + "," +

            ROCT * 0.01 + "," +
            RelBaroAltitudeT * 0.01 + "," +
            DesiredAltitudeT * 0.01 + "," +

            NorthPosET * 0.1 + "," +
            NavPCorrT * 0.001 + "," +
            EastPosET * 0.1 + "," +
            NavRCorrT * 0.001 + "," +

            HeadingT * 0.001 + "," +
            NavYCorrT + "," +

            MPU6XXXTempT * 0.1 + ",");

            SaveTextLogFileStreamWriter.Write("Stats,");

            for (i = 0; i < 22; i++)
            SaveTextLogFileStreamWriter.Write( Stats[i] + ",");

            for (i = 22; i < MaxStats; i++)
                SaveTextLogFileStreamWriter.Write(Stats[i]*0.001 + ",");

            SaveTextLogFileStreamWriter.Write( AirframeT + "," + OrientT + ",");

            if (TuningPacketsSeen) { 
                SaveTextLogFileStreamWriter.Write("Tune, " + TuneStateT + ",", TauT * 0.001 + "," + FCT * 0.01, ",");
      for (i = 0; i < 3; i++) {
                    SaveTextLogFileStreamWriter.Write( BetaT[i] * 0.001  + ",");          
                    SaveTextLogFileStreamWriter.Write(BiasT[i] * 0.001 + ",");
                    SaveTextLogFileStreamWriter.Write(NoiseT[i] * 0.001 + ",");
                    SaveTextLogFileStreamWriter.Write(OffsetT[i] * 0.001 + ",");
                    SaveTextLogFileStreamWriter.Write(KpaT[i] * 0.01 + ",");
                    SaveTextLogFileStreamWriter.Write(KiaT[i] * 0.01 + ",");
                    SaveTextLogFileStreamWriter.Write(KprT[i] * 0.01 + ",");
                    SaveTextLogFileStreamWriter.Write(KirT[i] * 0.01 + ",");
                    SaveTextLogFileStreamWriter.Write(KdrT[i] * 0.01 + ",");
      }
            }
            SaveTextLogFileStreamWriter.WriteLine();
        }

        void CloseFiles() {

            SaveLogFileBinaryWriter.Flush();
            SaveLogFileBinaryWriter.Close();
            SaveLogFileStream.Close();

            SaveTextLogFileStreamWriter.Flush();
            SaveTextLogFileStreamWriter.Close();
            SaveTextLogFileStream.Close();

            SaveTextInertialLogFileStreamWriter.Flush();
            SaveTextInertialLogFileStreamWriter.Close();
            SaveTextInertialLogFileStream.Close();
        }

        void UAVXCloseTelemetry()
        {
            if (serialPort1.IsOpen)
            {
                serialPort1.Close();
                CloseFiles();
            }
        }

