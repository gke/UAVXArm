                if (F[i])
                    SaveTextLogFileStreamWriter.Write("1,");
                else
                    SaveTextLogFileStreamWriter.Write(",");

            SaveTextLogFileStreamWriter.Write(StateT + "," +

            BatteryVoltsT * 0.1 + "," +
            BatteryCurrentT * 0.1 + "," +

            BatteryChargeT + "," +
            RCGlitchesT + "," +
            DesiredThrottleT * 0.001+ "," +
