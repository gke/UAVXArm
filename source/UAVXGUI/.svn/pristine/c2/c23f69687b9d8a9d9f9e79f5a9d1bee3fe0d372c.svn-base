        static int PreviousmS = 0;

                  static bool FirstInertial = true;

        void WriteTextInertialLogFile()
        {
 
            short k;
                double f;

            if (FirstInertial) { 
                if (DFTFreq > 0) {
                    FirstInertial = false;
                    f = Convert.ToDouble(DFTFreq);
                    for (k = 0; k < 8 ; k++)
                    {
                        SaveTextInertialLogFileStreamWriter.Write(f + "Hz,");
                        f *= 0.5;
                    }

                    SaveTextInertialLogFileStreamWriter.WriteLine();
                }
            } else {
                SaveTextInertialLogFileStreamWriter.Write(MissionTimeMilliSecT * 0.001 + "," +
                    (MissionTimeMilliSecT - PreviousmS) * 0.001 + "," +
                    GPSNoOfSatsT + "," +
                    GPSFixT + "," +
                    GPShAccT * 0.01f + "," +
                    GPSCFKpT + "," +
                    AltCFKpT + ",");

                for (k = 0; k < 20; k++)
                    SaveTextInertialLogFileStreamWriter.Write(Inertial[k] + ",");

                for (k = 7; k >= 0; k--)
                    SaveTextInertialLogFileStreamWriter.Write(DFT[k] * 0.001 + ",");

                SaveTextInertialLogFileStreamWriter.WriteLine();

                PreviousmS = MissionTimeMilliSecT; 
            }
        }

