        void ExtractFlags()
        {
            int bb, i, cf;

            for (i = 0; i < 6; i++)
            {
                cf = Flags[i];
                for (bb = 7; bb >= 0; bb--)
                    F[i * 8 + bb] = ((cf >> bb) & 1) != 0;
            }
        } // ExtractFlags

